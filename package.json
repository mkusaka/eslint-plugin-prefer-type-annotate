{
  "version": "0.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsc --watch src",
    "build": "rm -rf dist && tsc -p .",
    "test": "jest --passWithNoTests",
    "tc": "tsc -p . --noEmit",
    "lint": "eslint",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "@mkusaka/eslint-plugin-prefer-type-annotate",
  "author": "mkusaka",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/eslint": "^7.2.6",
    "@types/estree": "^0.0.46",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkusaka/eslint-plugin-prefer-type-annotate.git"
  },
  "bugs": {
    "url": "https://github.com/mkusaka/eslint-plugin-prefer-type-annotate/issues"
  },
  "homepage": "https://github.com/mkusaka/eslint-plugin-prefer-type-annotate",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.14.0"
  }
