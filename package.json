{
  "version": "0.0.3-beta.2",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc --watch src",
    "build": "rm -rf dist && tsc -p .",
    "test": "jest --passWithNoTests --runTestsByPath src/**/*.test.ts",
    "lint": "eslint",
    "prepare": "yarn build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "@mkusaka/eslint-plugin-prefer-type-annotate",
  "author": "mkusaka",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/eslint": "^6.8.1",
    "@types/estree": "^0.0.44",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkusaka/eslint-plugin-prefer-type-annotate.git"
  },
  "bugs": {
    "url": "https://github.com/mkusaka/eslint-plugin-prefer-type-annotate/issues"
  },
  "homepage": "https://github.com/mkusaka/eslint-plugin-prefer-type-annotate",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^3.1.0"
  }
}
